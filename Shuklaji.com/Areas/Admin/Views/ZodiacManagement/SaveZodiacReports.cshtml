
@{
    ViewData["Title"] = "SaveZodiacReports";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/js/Area/Admin/ZodiacManagement/SaveZodiacReports.js"></script>
<link href="~/css/Area/Admin/ZodiacManagement/ZodiacReport.css" rel="stylesheet" />
<div class="breadcrumb-data-1">
    <div class="row">
        <div class="col-md-6 col-5">
            <div class="breadcrumb-data-3">
                <h3 _ngcontent-hai-c46="">Save Zodiac Report</h3>
            </div>
        </div>
        <div class="col-md-6 col-7">
            <div class="breadcrumb-data-2 d-none d-sm-block">
                <ul>
                    <li><a>Home</a></li>
                    <li><span>/</span></li>
                    <li><a href="/admin/ZodiacManagement/ZodiacReport">Zodiac Management</a></li>
                    <li><span>/</span></li>
                    <li><p>Save Zodiac Report</p></li>
                </ul>
            </div>
        </div>
    </div>
</div>
<div class="data-container">
    <div class="inner-container">
        <form id="frmAddZodiacReport">
            <div class="container">
                <div class="row">
                    <div class="col-md">
                        <div class="dx-fieldset">
                            <div class="dx-field">
                                <div class="dx-field-label">Zodiac<strong class="mandatory">*</strong></div>
                                <div class="dx-field-value">
                                    <div id="ddlZodiac"></div>
                                </div>
                            </div>
                        </div>
                        <div class="dx-fieldset">
                            <div class="dx-field">
                                <div class="dx-field-label">Report Type<strong class="mandatory">*</strong></div>
                                <div class="dx-field-value">
                                    <div id="ddlReportType"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="dx-fieldset">
                            <div class="dx-field">
                                <div class="dx-field-label">Visible From<strong class="mandatory">*</strong></div>
                                <div class="dx-field-value">
                                    <div id="txtRangeFrom"></div>
                                </div>
                            </div>
                        </div>
                        <div class="dx-fieldset">
                            <div class="dx-field">
                                <div class="dx-field-label">Visible To<strong class="mandatory">*</strong></div>
                                <div class="dx-field-value">
                                    <div id="txtRangeTo"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg">
                        <div class="dx-fieldset">
                            <div class="html-editor"></div>
                            </div>
                        </div>
                    </div>
                <div class="row">
                    <div class="col-lg">
                        <div class="dx-fieldset">
                            <div class="dx-field">
                                <div class="dx-field-label"></div>
                                <div class="dx-field-value">
                                    <div id="btnSave"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </form>
    </div>
</div>


